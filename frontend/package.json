	},
	"dependencies": {
		"@vitejs/plugin-vue": "^4.4.0",
		"autoprefixer": "^10.4.19",
		"feather-icons": "^4.29.1",
		"frappe-ui": "0.1.105",
		"postcss": "^8.4.5",
		"socket.io-client": "^4.7.2",
		"tailwindcss": "^3.4.3",
		"vite": "^5.4.10",
		"vue": "^3.5.12",
		"vue-router": "^4.3.2"
	},
	"devDependencies": {
		"eslint": "^8.39.0",
		"eslint-plugin-vue": "^9.11.0",
		"prettier": "^2.8.8"
	}
}
