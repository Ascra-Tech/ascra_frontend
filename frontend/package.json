{
	"name": "frappe-ui-frontend",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build --base=/assets/ascra_frontend/ && yarn copy-html-entry",
		"copy-html-entry": "cp ../ascra_frontend/public/index.html ../ascra_frontend/www/frontend.html",
