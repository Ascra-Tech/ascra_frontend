{
	"name": "frappe-ui-frontend",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build --base=/assets/ascra_frontend/frontend/ && yarn copy-html-entry",
		"copy-html-entry": "cp ../ascra_frontend/public/frontend/index.html ../ascra_frontend/www/frontend.html",
		"preview": "vite preview",
		"lint": "biome check --write .",
		"test": "vitest",
		"test:run": "vitest run",
		"test:coverage": "vitest run --coverage"
	},
	"dependencies": {
		"feather-icons": "^4.29.2",
		"frappe-ui": "^0.1.150",
		"socket.io-client": "^4.7.2",
		"vue": "^3.4.0",
		"vue-router": "^4.3.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.8.0",
		"@vitejs/plugin-vue": "^4.6.0",
		"@vitest/coverage-v8": "^1.6.0",
		"autoprefixer": "^10.4.2",
		"jsdom": "^22.1.0",
		"postcss": "^8.4.5",
		"tailwindcss": "^3.3.0",
		"vite": "^4.5.0",
		"vitest": "^1.6.0"
	}
}
